// mojtest

program mojtest
{
	void main()	
	int x, y, z, pt;
	{	
		x = 0;
		y = 3;
		z = 3;
		pt = switch(x){
			case 0:
				x = 2;
				yield switch(x){
						default: yield 1000;
					};
				y++;
			case 1:
				y = 2;
				y++;
			case 3:
				z = 2;
				y++;
			case 4:
				x++;
				y++;
			default:
				yield x + z;
				z++;
		};
		
		y = 100;
		
		print(pt);
	}
}


